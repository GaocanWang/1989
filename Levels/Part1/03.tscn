[node name="2" type="Node" parent="PartTransition"]

[node name="DialogText" type="Node" parent="PartTransition/2"]
script = ExtResource("9_hs5gw")
text = "Something feels off."
npc_info = ExtResource("10_45svh")

[node name="3" type="Node" parent="PartTransition"]

[node name="DialogText" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "Amelia..."
npc_info = ExtResource("9_1hxyw")

[node name="DialogText2" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "Ameliaaaaaaaaaa..."
npc_info = ExtResource("9_1hxyw")

[node name="DialogText3" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "(This voice...)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText4" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "(Why does it sound so familiar?)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText5" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "Hey, you good?"
npc_info = ExtResource("9_1hxyw")

[node name="DialogText6" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "Hellllooooooooooooooooo?"
npc_info = ExtResource("9_1hxyw")

[node name="DialogText7" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "Anyone in there?"
npc_info = ExtResource("9_1hxyw")

[node name="DialogText8" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "(Was I dreaming this whole time...?)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText9" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "(Who's speaking to me...?)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText10" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "..."
npc_info = ExtResource("10_45svh")

[node name="DialogText11" type="Node" parent="PartTransition/3"]
script = ExtResource("9_hs5gw")
text = "You take a deep breath."
npc_info = ExtResource("10_45svh")

[node name="4" type="Node" parent="PartTransition"]

[node name="DialogText" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Amelia, are you still with us?"
npc_info = ExtResource("12_rpyh4")

[node name="DialogText2" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "You immediately recognize Teresa, your assigned partner for investigations and closest friend."
npc_info = ExtResource("10_45svh")

[node name="DialogText3" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "You've been serving in the same department for a little over six months now"
npc_info = ExtResource("10_45svh")

[node name="DialogText4" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "..."
npc_info = ExtResource("11_e47pi")

[node name="DialogText5" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Hey, Teresa."
npc_info = ExtResource("11_e47pi")

[node name="DialogText6" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Ah, welcome back to earth."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText7" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "You've been spacing out like that all week, you know?"
npc_info = ExtResource("12_rpyh4")

[node name="DialogText8" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Are you sure you're up for this case? If you're nt feeling well, I can always call in someone else."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText9" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "...I'm alright."
npc_info = ExtResource("11_e47pi")

[node name="DialogText10" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "I was daydreaming, that's about it. Did I miss much?"
npc_info = ExtResource("11_e47pi")

[node name="DialogText11" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Dreaming on the job? Now that's a new one."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText12" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "No, you didn't miss much. We were just discussing which area we want to start examining first."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText13" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "Everyone evacuated earlier when we got the dispatch, so we have the whole place to ourselves."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText14" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "We better act quick though."
npc_info = ExtResource("12_rpyh4")

[node name="DialogText15" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "(Right, the case.)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText16" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "(That's why I'm here after all.)"
npc_info = ExtResource("11_e47pi")

[node name="DialogText17" type="Node" parent="PartTransition/4"]
script = ExtResource("9_hs5gw")
text = "(I guess it's time to put my attention to where it really matters now and solve this case quickly."
npc_info = ExtResource("11_e47pi")

