[node name="Kickboards" parent="." instance=ExtResource("11_le5b1")]
position = Vector2(156, 179)
size = Vector2(18, 18)

[node name="DialogText" type="Node" parent="Kickboards"]
script = ExtResource("7_8ygk6")
text = "A bin full of kickboards."
npc_info = ExtResource("12_7aieg")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Kickboards"]
script = ExtResource("7_8ygk6")
text = "“Doesn’t look like anyone uses these.”"
frame = 13
npc_info = ExtResource("10_t3jo2")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Kickboards"]
script = ExtResource("7_8ygk6")
text = "“It’s full to the brim.”"
frame = 22
npc_info = ExtResource("10_t3jo2")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Kickboards"]
script = ExtResource("7_8ygk6")
text = "“We always have the pool equipment checkout log.”"
frame = 10
npc_info = ExtResource("13_iie8u")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Towels" parent="." instance=ExtResource("11_le5b1")]
position = Vector2(228, 74)

[node name="DialogText" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "A stack of seven neatly folded pool towels."
npc_info = ExtResource("12_7aieg")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“Hold on.”"
frame = 10
npc_info = ExtResource("9_khc4k")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“Is it possible one of the community center towels was used to wipe the blood?”"
frame = 8
npc_info = ExtResource("9_khc4k")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“That’s what I was thinking too.”"
frame = 5
npc_info = ExtResource("13_iie8u")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“It would be risky to clean up blood with your own towel.”"
npc_info = ExtResource("13_iie8u")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“But with a public towel, it’d be harder to trace back to you.”"
frame = 4
npc_info = ExtResource("13_iie8u")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“Then we just have to check whoever took a community towel today.”"
frame = 19
npc_info = ExtResource("10_t3jo2")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="Towels"]
script = ExtResource("7_8ygk6")
text = "“Whoever was listed taking a towel is a potential suspect.”"
frame = 5
npc_info = ExtResource("10_t3jo2")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

