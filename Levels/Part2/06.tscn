[node name="DialogText" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "”Whoa, this looks like the staff lounge.”"
npc_info = ExtResource("8_q4xlc")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“You’ve got cushions and couches.”"
npc_info = ExtResource("8_q4xlc")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“There's quite a few places in here we can check for the key.”"
npc_info = ExtResource("9_5gb84")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“I can’t imagine any other place to lose a key.”"
npc_info = ExtResource("9_5gb84")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“It’s got to be in here.”"
npc_info = ExtResource("10_j788d")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“Then let’s start looking.”"
npc_info = ExtResource("8_q4xlc")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="."]
script = ExtResource("7_q4xlc")
text = "“…”"
npc_info = ExtResource("11_iv8hn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

