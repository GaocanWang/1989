[node name="DialogText" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“I’ve already checked everywhere, hon.”"
frame = 30
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Don’t think this place has anything we can use.\""
frame = 19
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Honestly, this place disgusts me.”"
frame = 37
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“I’ve never been here before.”"
frame = 4
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“To the bathroom?”"
frame = 9
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“No, this community center.”"
frame = 30
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Everything about it is just plain creepy.”"
frame = 28
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Why are all the lights so dim?”"
frame = 9
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Bleugh…doesn’t matter to me.”"
frame = 37
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="NPC/DialogInteraction"]
script = ExtResource("9_wvfgd")
text = "“Feel free to check around if you want.”"
frame = 14
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Repeat" type="Node" parent="NPC/DialogInteraction"]

[node name="DialogText" type="Node" parent="NPC/DialogInteraction/Repeat"]
script = ExtResource("9_wvfgd")
text = "“What do you need, Amelia?”"
frame = 26
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogChoice" type="Node" parent="NPC/DialogInteraction/Repeat"]
