metadata/_custom_type_script = "uid://cm6c0rnohy8s1"

[node name="DialogText" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“I want to talk about X.”"
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“About X?”"
frame = 30
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“Oh that old grouch.”"
frame = 14
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“He might seem cold but he’s just always like that.”"
frame = 8
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“I’m clearly the better one in this duo.”"
frame = 18
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch"]
script = ExtResource("9_wvfgd")
text = "“Uh huh…”"
frame = 9
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBranch2" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice"]
script = ExtResource("13_5cgqs")
text = "I want to talk about you"
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://cm6c0rnohy8s1"

[node name="DialogText" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“I want to talk about you.”"
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“What about me?”"
frame = 26
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Just your background and all.”"
frame = 16
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Do you always wear perfume and gloves?”"
frame = 15
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Oh, so you noticed!”"
frame = 10
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Not often, if I’m being honest with myself.”"
frame = 14
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“But I just had to dress up nice for you, great detective.”"
frame = 18
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Right…”"
frame = 9
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“I’m joking, hon!”"
frame = 8
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“I just believe in the power of looks.”"
frame = 14
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Appearance is everything in a world full of watchers.”"
frame = 13
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“You got a boyfriend, Amelia?”"
frame = 18
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Wha…!”"
frame = 4
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText14" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“No…?”"
frame = 3
npc_info = ExtResource("11_n0ty7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText15" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“You’ve much to learn about the world, hon.”"
frame = 26
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText16" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Besides observation, experience builds a great detective.”"
frame = 8
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText17" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "Bit of advice for you…”"
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText18" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“…your looks matter as much as your evidence.”"
frame = 14
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText19" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“The right evidence with the wrong outfit will be overlooked.”"
frame = 4
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText20" type="Node" parent="NPC/DialogInteraction/Repeat/DialogChoice/DialogBranch2"]
script = ExtResource("9_wvfgd")
text = "“Now shoo.”"
frame = 18
npc_info = ExtResource("10_d1f0s")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

