[node name="DialogText3" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“There’s something down there.”"
frame = 15
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“It’s stuck under the mesh…just beneath the grate.”"
frame = 10
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“Can you reach it?”"
frame = 30
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“Not at all.”"
frame = 18
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“Whatever it is, it’s near the bottom.”"
frame = 17
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“It looks a little bit like a key.”"
frame = 19
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "If it was just a little closer to the top…”"
frame = 16
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="Drain/1"]
script = ExtResource("7_5lixq")
text = "“I’d probably be able to fish it out.”"
frame = 15
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="2" type="Node" parent="Drain"]

[node name="DialogText" type="Node" parent="Drain/2"]
