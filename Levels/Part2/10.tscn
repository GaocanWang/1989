[node name="DialogText" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Do you hear that?”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Yeah.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“It sounds like something rattling inside the walls.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“All these pipes, valves, and pressure systems… the noise could be coming from anywhere.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“This must be the maintenance room.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“It’s kind of obnoxious.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“I wouldn’t last ten minutes working in here.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“It’s rhythmic…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Whatever’s making that sound…it’s moving with the flow of the water.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Like it’s caught in the system.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Do you think it could be the key?”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText14" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Could be.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText15" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“But how a key lands inside the draining system is beyond me.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText16" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Either way, we won’t know until we check.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText17" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Let’s search the room then.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText18" type="Node" parent="."]
script = ExtResource("7_5lixq")
text = "“Let’s figure out what’s making that noise.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Drain" parent="." instance=ExtResource("12_trw21")]
position = Vector2(204, 132)

[node name="DialogText" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "A square floor drain sits at the lowest point in the room."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "Through the mesh grate, something faintly glints beneath the water."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Hold on…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“There’s something down there.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "”Hold on a sec…that’s the key!”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“You’re right.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“It’s stuck under the mesh…just beneath the grate.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Can you reach it?”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Not at all.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“The grate’s screwed down tight, and I don’t have anything thin enough to pry it open.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“There’s a small keyhole in the grate.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“If only we had the key…”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“…But then we wouldn’t need to open the grate.\""
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText14" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“We were so close!”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText15" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Is there no way we can get the key?”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText16" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“If only it was just a little closer…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText17" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“I’d probably be able to fish it out.”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText18" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“…”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText19" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“What if we pushed the key up?”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText20" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Push it up?”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText21" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“The key’s just sitting there, right?”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText22" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“If we could raise the water level…”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogSFX" type="Node" parent="Drain"]
script = ExtResource("14_jjkcr")
sfx = ExtResource("15_pra55")
metadata/_custom_type_script = "uid://dst30i4jr8cmt"

[node name="DialogText23" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“…It might float to the top!”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogSFX2" type="Node" parent="Drain"]
script = ExtResource("14_jjkcr")
metadata/_custom_type_script = "uid://dst30i4jr8cmt"

[node name="DialogText24" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“That might just work.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText25" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“But are we really about to flood the maintenance room?”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText26" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“Only just enough to bring the key within reach.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText27" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“I saw some valves in the back!”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText28" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“We might be able to control the water flow with them.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText29" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“If we redirect the water pressure into this section of the pipes…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText30" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“…We could raise the water level inside the drain!”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText31" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“It’s settled.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText32" type="Node" parent="Drain"]
script = ExtResource("7_5lixq")
text = "“We need to go check the meters.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Repeat" type="Node" parent="Drain"]

[node name="DialogText" type="Node" parent="Drain/Repeat"]
script = ExtResource("7_5lixq")
text = "A square floor drain sits at the lowest point in the room."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Drain/Repeat"]
script = ExtResource("7_5lixq")
text = "Through the mesh grate, the key faintly glints beneath the water."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Drain/Repeat"]
script = ExtResource("7_5lixq")
text = "(It’s still out of reach.)"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Drain/Repeat"]
script = ExtResource("7_5lixq")
text = "(We need to increase the water pressure and float it up.)"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Pipes" parent="." instance=ExtResource("12_trw21")]
position = Vector2(209, 161)
size = Vector2(60, 12)

[node name="DialogText" type="Node" parent="Pipes"]
script = ExtResource("7_5lixq")
text = "A slightly rusted pipe among many others."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Pipes"]
script = ExtResource("7_5lixq")
text = "The noise doesn’t appear to be coming from inside these pipes."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Pipes"]
script = ExtResource("7_5lixq")
text = "(Not here…)"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="PoolFilter" parent="." instance=ExtResource("12_trw21")]
position = Vector2(132, 84)

[node name="DialogText" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "A large cylindrical tank stands here."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "It appears to be the pool filter."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "“These capture small things in the pool, right?”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "“Yeah, but the keys wouldn’t be here.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "“These only capture small debris that float in the water.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="PoolFilter"]
script = ExtResource("7_5lixq")
text = "“The keys would sink.”"
npc_info = ExtResource("10_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Chlorinator" parent="." instance=ExtResource("12_trw21")]
position = Vector2(252, 156)

[node name="DialogText" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "A compact machine connected to the pipes."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "A label reads ‘Chlorinator’ in faded text."
npc_info = ExtResource("13_ek7bd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“We won’t find the keys here either.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“It only releases chlorine at set levels.”"
npc_info = ExtResource("11_i1rcq")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“…”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“What’s it set to right now?”"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“Right now?”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“Well they don’t change these often I think.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“It’s set to normal.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "(It’s only set to normal?)"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "(Then why does…?)"
npc_info = ExtResource("8_mgodn")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="ValveInstructions" parent="." instance=ExtResource("12_trw21")]
position = Vector2(168, 60)

[node name="DialogText" type="Node" parent="ValveInstructions"]
script = ExtResource("7_5lixq")
