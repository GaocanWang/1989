[node name="DialogText8" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
text = "“It’s set to normal.”"
npc_info = ExtResource("9_trw21")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="Chlorinator"]
script = ExtResource("7_5lixq")
