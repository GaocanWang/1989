metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Cabinet/2"]
script = ExtResource("7_t1jd6")
text = "“This looks really promising.”"
frame = 2
npc_info = ExtResource("8_8x0r1")
