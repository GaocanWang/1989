[node name="DialogText2" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“Hey, what are you doing?”"
frame = 9
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“There’s a ton of these fragments.”"
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“I’m just taking one in case I need it.”"
frame = 15
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“You know that’s against protocol.”"
frame = 15
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“Forensics won’t be able to arrive in time.”"
frame = 14
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="SinkPieces/Yes"]
script = ExtResource("10_1nj6t")
text = "“I’m just collecting it for them.”"
frame = 2
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

