[node name="Check" parent="NPC" instance=ExtResource("15_yjpjn")]

[node name="DialogText" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“I’ve been looking at these broken fragments for a while.”"
frame = 11
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“Apart from the sink, the mirror is broken too.”"
frame = 13
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“All this blood and mirror splatter makes no sense.”"
frame = 12
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“Do you think it’s related to the lifeguard?”"
frame = 15
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "\"I hope not.”"
frame = 1
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“…Damn it.”"
frame = 15
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“Where did that lifeguard go?”"
frame = 9
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“This case makes no sense.”"
frame = 17
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“No lifeguard.”"
frame = 18
npc_info = ExtResource("14_imhlx")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“And…no leak.”"
frame = 4
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“I checked around for any sign of a burst pipe or something.”"
frame = 9
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“But there’s no leak whatsoever.”"
frame = 3
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="NPC/Check"]
script = ExtResource("10_1nj6t")
text = "“Which means he couldn’t have gotten knocked unconscious.”"
frame = 1
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Repeat" type="Node" parent="NPC/Check"]

[node name="DialogText" type="Node" parent="NPC/Check/Repeat"]
script = ExtResource("10_1nj6t")
text = "“You should go find Teresa.”"
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="NPC/Check/Repeat"]
script = ExtResource("10_1nj6t")
text = "“She said something about waiting for you.”"
frame = 6
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="NPC/Check/Repeat"]
script = ExtResource("10_1nj6t")
text = "“I need some time to think anyway…”"
frame = 10
npc_info = ExtResource("18_vkprd")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"
