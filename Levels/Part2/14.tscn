[node name="DialogText2" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Why’s there so much steam in the changeroom?”"
frame = 17
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“It sounds like some of the showers are on.”"
frame = 26
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="SavePoint" parent="DialogText3" instance=ExtResource("10_tiff0")]
position = Vector2(36, 254)

[node name="DialogText4" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Maybe that’s why.”"
frame = 19
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Why would the showers be on?”"
frame = 19
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Is the lifeguard here?”"
frame = 5
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“HEY!”"
frame = 7
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“IS ANYONE IN HERE?”"
frame = 9
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "..."
slow = true
npc_info = ExtResource("12_rp4a4")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“It doesn’t sound like he’s in here.”"
frame = 22
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText11" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Let’s go check.”"
frame = 4
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Wait!”"
frame = 7
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“There’s something I need to do.”"
frame = 18
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText14" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "You take a deep breath to calm yourself."
npc_info = ExtResource("12_rp4a4")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText15" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "(Here goes nothing.)"
frame = 17
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText18" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "(…)"
frame = 18
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText19" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“I want you to let me explore the changeroom first.”"
frame = 15
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText20" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Alone.”"
frame = 14
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText21" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“You can’t be serious!”"
frame = 26
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText22" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Why would you need to do that?”"
frame = 4
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText23" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“We can’t just let you go in by yourself, Amelia.”"
frame = 12
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText24" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“We don’t know what’s in there.”"
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText25" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“There might be important evidence.”"
frame = 15
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText26" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Can’t we just follow your lead?”"
frame = 2
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText28" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“I need to be by myself.”"
frame = 15
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText29" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“I work best uninterrupted when it comes to things like this.”"
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText30" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“This room…there’s something not right.”"
frame = 16
npc_info = ExtResource("8_pjrq6")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText31" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“You can’t just act like you’re in charge of this case.”"
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText32" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“We’re wasting time.”"
frame = 37
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText34" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Hold on.”"
frame = 1
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText36" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“You can’t be seriously considering her idea!”"
frame = 26
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText38" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“She does this a lot.”"
frame = 2
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText39" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Just let it happen.”"
frame = 3
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText40" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Alright, Amelia.”"
frame = 4
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText41" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“You’ve got to show us that we can trust you.”"
frame = 12
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText42" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Looks like Teresa’s already on your side.\""
frame = 10
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText43" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“If you can convince DAISY and I, then we’ll all agree to let you go inside.”"
frame = 2
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText44" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“That won’t be easy.”"
frame = 4
npc_info = ExtResource("9_etbsa")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText45" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Hey, Amelia…”"
frame = 3
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground" type="Node" parent="."]
script = ExtResource("13_hxc6f")
background = ExtResource("14_e5rgf")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText46" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“All they’re gonna do is ask you questions.”"
frame = 2
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground2" type="Node" parent="."]
script = ExtResource("13_hxc6f")
background = ExtResource("15_7r2dw")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText47" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Think about it, then answer wisely.”"
frame = 4
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground3" type="Node" parent="."]
script = ExtResource("13_hxc6f")
background = ExtResource("16_khpv5")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText48" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Try not to say the wrong thing too many times.”"
frame = 19
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground4" type="Node" parent="."]
script = ExtResource("13_hxc6f")
background = ExtResource("17_8cnjf")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText49" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“If you do that and can’t answer in time, they won’t be convinced.”"
frame = 5
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground5" type="Node" parent="."]
script = ExtResource("13_hxc6f")
background = ExtResource("18_faqmo")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText50" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“If your answers are good enough, they shouldn’t have any objections left.”"
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground6" type="Node" parent="."]
script = ExtResource("13_hxc6f")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText51" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“You got this.”"
frame = 1
npc_info = ExtResource("10_1bov7")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText52" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“I’ll give you a minute to collect your thoughts.”"
frame = 5
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText53" type="Node" parent="."]
script = ExtResource("7_cjict")
text = "“Come to me when you’re ready.”"
frame = 6
npc_info = ExtResource("11_0yrxr")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

