frame = 25
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText35" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“You’re not…”"
frame = 23
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText36" type="Node" parent="Check"]
script = ExtResource("6_22iej")
