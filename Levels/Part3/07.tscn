metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="."]
script = ExtResource("6_22iej")
text = "“It’s you…”"
frame = 4
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="."]
script = ExtResource("6_22iej")
text = "“No, it’s not.”"
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="."]
script = ExtResource("6_22iej")
text = "“Don’t confuse me for someone else.”"
frame = 16
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="."]
script = ExtResource("6_22iej")
text = "(What…?)"
frame = 19
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="Check" parent="." instance=ExtResource("9_82jsk")]
position = Vector2(222, 132)
size = Vector2(36, 24)

[node name="DialogText" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“…”"
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText2" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“There’s a story about a sailor.”"
frame = 15
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText3" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Care to hear it?”"
frame = 10
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText4" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Where am I?”"
frame = 4
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText5" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“What did you do?”"
frame = 37
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText6" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“You know this one already.”"
frame = 8
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText7" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“They say he left for all kinds of reasons.”"
frame = 9
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText8" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Maybe he wanted time alone. Maybe he was chasing something.”"
frame = 10
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText9" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“But everyone remembered one thing…”"
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText10" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“…he promised to return with fish. Lots of them.”"
frame = 15
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground" type="Node" parent="Check"]
script = ExtResource("10_jt5el")
background = ExtResource("11_ehuje")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogSFX" type="Node" parent="Check"]
script = ExtResource("12_51uwe")
sfx = ExtResource("13_ehuje")
metadata/_custom_type_script = "uid://dst30i4jr8cmt"

[node name="DialogText11" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“He brought every net he owned. Every harpoon.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText12" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“The sailor insisted he wouldn’t come back until he’s killed every fish in the sea.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText13" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“And so he left.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText14" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“…”"
slow = true
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText15" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“The sailor didn’t come back.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText16" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Years passed. People forgot his name.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText17" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Until one day, his body washed ashore.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText18" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“…with a harpoon stuck in him.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText19" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“It seemed like an accident.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText20" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“But some argued he went mad.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText21" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“That he turned the harpoon on himself when he realized the sea was too big.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText22" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Too deep.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText23" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Too full of fish he couldn’t kill.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText24" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "..."
slow = true
npc_info = ExtResource("15_foabf")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText25" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“No one knows for sure.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText26" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“To this day, it remains a mystery.”"
npc_info = ExtResource("14_51uwe")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogBackground2" type="Node" parent="Check"]
script = ExtResource("10_jt5el")
metadata/_custom_type_script = "uid://6cyylp4dfkoi"

[node name="DialogText27" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Then what’s the point of the story?”"
npc_info = ExtResource("7_82jsk")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText28" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“The point, DAISY…”"
frame = 14
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText29" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“…is that only the sailor knows the truth.”"
frame = 1
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText30" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“Only he knew what he was running from.”"
frame = 2
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText31" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“And only he knew why he never came back.”"
frame = 15
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogText32" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“That truth died with him.”"
frame = 8
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogSFX2" type="Node" parent="Check"]
script = ExtResource("12_51uwe")
metadata/_custom_type_script = "uid://dst30i4jr8cmt"

[node name="DialogText33" type="Node" parent="Check"]
script = ExtResource("6_22iej")
text = "“And that is his burden to carry.”"
slow = true
frame = 1
npc_info = ExtResource("8_rw525")
metadata/_custom_type_script = "uid://dqrok5vg7oa3d"

[node name="DialogSFX3" type="Node" parent="Check"]
